            text = ""
        intervals [643]:
            xmin = 3742.880000
            xmax = 3757.120000
            text = "I really detest this terminology because when you speak of provability, a notion of provability, you imply that there is a corresponding notion of proof or this is a psychological implication."
        intervals [644]:
            xmin = 3757.120000
            xmax = 3757.600000
